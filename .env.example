# App Environment
NODE_ENV=development
APP_NAME='Ucartz Frontend'
APP_VERSION=1.0.0

# Base API URL (used across environments)
REACT_APP_API_BASE_URL=https://api.example.com
# Example: http://localhost:4000/api
# Example: https://api.example.com

# WebSocket / Reverb / Socket connection
REACT_APP_WS_URL=wss://ws.example.com

# Public URL for assets (optional)
PUBLIC_URL=/

# Debugging flags
REACT_APP_ENABLE_LOGS=true

# Google Maps
REACT_APP_GOOGLE_MAPS_API_KEY=
