# .env.example
# Example environment configuration for React applications
# Copy this file to .env and fill in real values for local development

# Application environment: dev, staging, or production
REACT_APP_ENVIRONMENT=dev

# Base API URL. Replace PORT with your backend's local port or use the deployed API domain.
REACT_APP_API_URL=http://localhost:PORT/api
# Example: http://localhost:4000/api
# Example: https://api.example.com

# The display name for your application
REACT_APP_APP_NAME=APP_NAME

# The current version of your app
REACT_APP_VERSION=1.0.0
